{
  "name": "@ultimatearcade/client-sdk",
  "version": "0.1.1",
  "description": "Ultimate Arcade JS Game Client SDK",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "build:clean": "rm -rf build && npm run build",
    "release": "npm run build:clean && standard-version",
    "release:major": "npm run build:clean && standard-version --release-as major",
    "release:minor": "npm run build:clean && standard-version --release-as minor",
    "release:patch": "npm run build:clean && standard-version --release-as patch",
    "release:alpha": "npm run build:clean && standard-version --prerelease alpha",
    "release:beta": "npm run build:clean && standard-version --prerelease beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UltimateTournament/ArcadeJSClientSDK.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UltimateTournament/ArcadeJSClientSDK/issues"
  },
  "homepage": "https://github.com/UltimateTournament/ArcadeJSClientSDK#readme",
  "dependencies": {
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "standard-version": "^9.2.0",
    "typescript": "^4.9.5"
  }
}
